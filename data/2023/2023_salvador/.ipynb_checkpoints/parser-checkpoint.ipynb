   "source": [
    "import json\n",
    "import pandas as pd"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "df8e8dc8-23ef-47a2-91eb-47c0e664ca1d",
   "metadata": {},
   "outputs": [],
   "source": [
    "files_to_read = !ls *.json"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "a3bdf9dc-8684-4df4-a48d-954dba884327",
   "metadata": {},
   "outputs": [],
   "source": [
    "all_data = []\n",
    "\n",
    "for file_to_read in files_to_read:\n",
    "    with open(file_to_read) as f:\n",
    "        d = json.load(f)\n",
    "        all_data.extend(d)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "2ed5b324-333a-4f48-bab8-0af67425623c",
   "metadata": {},
   "outputs": [],
   "source": [
    "pd.DataFrame(all_data).to_csv(\"out.csv\")"
   ]
  },
